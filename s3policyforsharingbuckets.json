{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::<DESTINATION ACCOUNT HERE>:user/<DESTINATION ACCOUNT USER HERE>"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:PutObjectAcl"
            ],
            "Resource": "arn:aws:s3:::<BUCKET IN SOURCE ACCOUNT HERE>"
        }
    ]
}
